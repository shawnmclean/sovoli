{
  "extends": "@sovoli/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "react-native": ["./node_modules/react-native-web"]
    }
  },
  "include": [
    "next-env.d.ts",
    "nativewind-env.d.ts",
    "next.config.mjs",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
