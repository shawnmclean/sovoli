{
  "extends": "@sovoli/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "jsxImportSource": "nativewind"
  },
  "include": [
    "next-env.d.ts",
    "app-env.d.ts",
    "nativewind.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
