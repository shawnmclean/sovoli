{
  "name": "@sovoli/sovoli.com",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf node_modules .next .turbo .trigger",
    "dev": "concurrently --raw --kill-others pnpm:dev:*",
    "dev:next": "pnpm with-env next dev --experimental-https",
