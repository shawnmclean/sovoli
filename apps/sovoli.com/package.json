    "deploy:trigger-prod": "trigger deploy",
    "deploy:trigger": "trigger deploy --env staging",
    "start": "pnpm with-env next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
