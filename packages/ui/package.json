    "class-variance-authority": "^0.7.0",
    "nativewind": "~4.0.36",
    "tailwind-merge": "^2.3.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@sovoli/typescript-config": "workspace:*",
    "@types/react": "^18.2.46",
    "@types/react-native": "^0.73.0",
    "typescript": "^5.3.3"
  }
